{
  "name": "footplr",
  "version": "0.0.1",
  "description": "A native application built with NativeScript-Vue",
  "author": "Eddy Verbruggen <eddyverbruggen@gmail.com>",
  "license": "MIT",
  "nativescript": {
    "id": "com.footballplayerratings.app",
    "tns-ios": {
      "version": "5.3.1"
    },
    "tns-android": {
      "version": "5.3.1"
    }
  },
  "scripts": {
    "tsc": "cd app && tsc --skipLibCheck --watch",
    "ios-and-android": "tns run --bundle",
    "ios": "tns run ios --bundle",
    "ios.release": "tns build ios --bundle --release --forDevice --provision 56140ef2-ea59-483c-8fe9-8438fc8a892c",
    "ios.emulator": "tns run ios --bundle --emulator",
    "android": "tns run android --bundle",
    "android.release": "tns build android --bundle --release --for-device --key-store-path ./keystore-footplr.jks --key-store-password xs4all --key-store-alias footplr --key-store-alias-password xs4all"
  },
  "dependencies": {
    "nativescript-appversion": "~1.4.2",
    "nativescript-camera": "~4.5.0",
    "nativescript-feedback": "~1.3.9",
    "nativescript-imagepicker": "~6.2.0",
    "nativescript-iqkeyboardmanager": "~1.5.1",
    "nativescript-numeric-keyboard": "~4.2.3",
    "nativescript-pager": "~9.5.4",
    "nativescript-plugin-firebase": "~9.0.3",
    "nativescript-taptic-engine": "~2.1.0",
    "nativescript-theme-core": "~1.0.6",
    "nativescript-vue": "~2.3.0",
    "nativescript-web-image-cache": "git+https://github.com/EddyVerbruggen/nativescript-web-image-cache.git#2b7a51a36a289b39132daabca045e92784a26d70",
    "tns-core-modules": "~5.4.3",
    "vue-hot-reload-api": "~2.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "babel-loader": "^8.0.6",
    "babel-traverse": "6.26.0",
    "babel-types": "6.26.0",
    "babylon": "6.18.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "cryptiles": "~4.1.3",
    "css-loader": "^3.0.0",
    "lazy": "1.0.11",
    "nativescript-dev-typescript": "~0.10.0",
    "nativescript-dev-webpack": "next",
    "nativescript-vue-template-compiler": "~2.3.0",
    "nativescript-worker-loader": "~0.9.5",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "terser-webpack-plugin": "^1.3.0",
    "tns-platform-declarations": "~5.4.3",
    "typescript": "~3.5.3",
    "vue-loader": "^15.7.0",
    "webpack": "^4.35.3",
    "webpack-bundle-analyzer": "~3.3.2",
    "webpack-cli": "^3.3.5"
  }
}
